{
  "types": ["feat", "fix", "docs", "style", "refactor", "test", "chore"],
  "scope": {
    required: false,
    allowed: ['button', 'card'],
    validate: false,
    multiple: false
  },
  "warnOnFail": false,
  "maxSubjectLength": 1500,
  "subjectPattern": ".+",
  "subjectPatternErrorMsg": "please check out guidelines for git commit pattern",
  "autoFix": false
}
