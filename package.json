{
  "name": "print-cli",
  "version": "1.1.0",
  "description": "print command line interface",
  "main": "./bin/print-cli.js",
  "directories": {
    "bin": "bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sridharmallela/print-cli.git"
  },
  "keywords": [
    "Print",
    "CLI"
  ],
  "bin": {
    "print-cli": "./bin/print-cli"
  },
  "author": "Sridhar Mallela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sridharmallela/print-cli/issues"
  },
  "homepage": "https://github.com/sridharmallela/print-cli#readme",
  "config": {
    "unsafe-perm": true,
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "test",
        "chore"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 1500,
      "helpMessage": "use karma style git commit pattern"
    }
  },
  "scripts": {
    "precommit": "npm test",
    "commitmsg": "./node_modules/.bin/validate-commit-msg",
    "lint": "./node_modules/.bin/eslint .",
    "clean": "./node_modules/.bin/rimraf ./bin/print-cli",
    "uglify": "./node_modules/.bin/uglifyjs ./lib/print-cli.js -c -m --warn -o ./bin/print-cli",
    "test-unit": "./node_modules/.bin/mocha test",
    "build": "npm run lint && npm run clean && npm run uglify",
    "prepublishOnly": "npm test",
    "help": "npm test && node ./bin/print-cli --help",
    "test": "npm run build && npm run test-unit",
    "start": "npm test && node ./bin/print-cli --banner \"PRINT-CLI\""
  },
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.11.0",
    "figlet": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "child_process": "^1.0.2",
    "eslint": "^4.5.0",
    "husky": "^0.14.3",
    "mocha": "^3.5.0",
    "path": "^0.12.7",
    "rimraf": "^2.6.1",
    "uglify-js": "^3.0.28",
    "validate-commit-message": "^3.0.1"
  }
}
